{
	"msg_broker":
	{
		"url": "@@MsgBrokerUrl@@"
		,"options":
		{
			"outgoingHeartBeatMS": 10000
			,"heartBeatScaleFactor": 0.8
		}
		,"connectHeaders":
		{
			"login": "@@MsgBrokerUserName@@"
			,"passcode": "@@MsgBrokerPassword@@"
		}
	}
	,"gridEventTopic": "/topic/grid_events"
	,"dispatcherConfig":
	{
		"protocol": "http/https"
		,"hostname": "@@DispatcherHostName@@"
		,"port": @@DispatcherPort@@
		,"verifySSLCertificates": true/false
	}
}